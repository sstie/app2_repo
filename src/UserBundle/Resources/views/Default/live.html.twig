{% extends '::base.html.twig' %}


{% block title %}Live-Karte{% endblock %}
{% block body %}
Live-Karte

<div class="content">
    <h2>Map</h2>
    <div id="maparea">MAP</div>
</div>

<div class="legend">
    <h2>Legend</h2>
    <ul>
        {% for device in devices %}
            <li class="legend_entry">{{ device.name|e }}</li>
        {% else %}
            <li class="legend_no_entry"><em>no device found</em></li>
        {% endfor %}
    </ul>

</div>

{% endblock %}

